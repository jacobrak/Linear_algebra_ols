    "import numpy as np\n",
    "np.array([])\n",
    "\n",
    "def beta_estimate(X, y):\n",
    "    X = np.array(X, dtype=float)\n",
    "    y = np.array(y, dtype=float)\n",
    "    return np.linalg.inv(X.T @ X) @ X.T @ y"
